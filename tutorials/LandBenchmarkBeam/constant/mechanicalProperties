planeStress     no;

mechanical
(
    heartTissue
    {
        type            electroMechanicalLaw;
        rho             rho [1 -3 0 0 0 0 0] 3000;
        activeTension   activeTension [1 -1 -2 0 0 0 0] 0.0;
        passiveMechanicalLaw
        {
            type            GuccioneElastic;
            k               K [1 -1 -2 0 0 0 0] 2e3;
            cf              8.0;
            ct              2.0;
            cfs             4.0;

            // The bulk modulus acts as a penalty factor to enforce
            // incompressiblity
            bulkModulus     bulkModulus [1 -1 -2 0 0 0 0] 200e3; //20e3;
        }

        // type            neoHookeanElastic;
        // rho             rho [1 -3 0 0 0 0 0] 7800;
        // mu              mu [1 -1 -2 0 0 0 0] 400e+6;
        // K               K [1 -1 -2 0 0 0 0] 10e+9;

