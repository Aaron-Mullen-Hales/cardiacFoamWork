FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Vm;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform -0.08523;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

