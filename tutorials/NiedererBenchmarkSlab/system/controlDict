FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     electroActivationFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.15;

// Three time-steps are used:

// deltaT = 0.05 ms
// deltaT          0.05e-3;

// deltaT = 0.01 ms
// deltaT          0.01e-3;

// deltaT = 0.005 ms
deltaT          0.005e-3;

writeControl    runTime;

writeInterval   5e-3;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable false;


// ************************************************************************* //
