"nonLinearGeometryTotalLagrangianTotalDisplacementCoeffs|nonLinearGeometryTotalLagrangianCoeffs|nonLinearGeometryUpdatedLagrangianCoeffs|unsNonLinearGeometryUpdatedLagrangianCoeffs|unsNonLinearGeometryTotalLagrangianCoeffs"
{
    // Linear predictor at the start of each time-step
    //predictor               yes;

    // Maximum number of momentum correctors
    nCorrectors             10000;

    // Solution tolerance for displacement
    solutionTolerance       1e-06;

    // Alternative solution tolerance for displacement
    alternativeTolerance    1e-12; // force use of solutionTolerance

    // Material law solution tolerance
    //materialTolerance       1e-05;

    // Write frequency for the residuals
    infoFrequency           100;

    stabilisation
    {
        //type         none;
        type         RhieChow;
        scaleFactor  1.0;
    }
}

