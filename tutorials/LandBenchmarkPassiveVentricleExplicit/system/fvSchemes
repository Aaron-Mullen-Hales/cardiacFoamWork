FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default            none; //Euler; //steadyState;
}

ddtSchemes
{
    //default            steadyState;
    default            none; //Euler; // for predictor
    ddt(epsilonVol) Euler;
}

gradSchemes
{
    default            pointCellsLeastSquares;
    //default            faceLimited pointCellsLeastSquares 1;
    //default            cellLimited pointCellsLeastSquares 1;
    //default            edgeCellsLeastSquares;
}

divSchemes
{
    default            Gauss linear;
}

laplacianSchemes
{
    default            Gauss linear skewCorrected;
    // default            Gauss linear limited 0.5;
}

snGradSchemes
{
    default            skewCorrected;
    //default            limited 0.5;
}

interpolationSchemes
{
    default            skewCorrected linear;
}

